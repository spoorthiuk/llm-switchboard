{
  "name": "llm-switchboard",
  "publisher": "LLM-Nexus", 
  "displayName": "LLM Switchboard",
  "description": "Offline code assistant for VS Codeâ€”seamlessly switch between multiple AI models to enhance productivity and creativity, all without needing an internet connection.",
  "version": "0.0.3",
  "icon": "media/logo.png",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {

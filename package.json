    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@types/prismjs": "^1.26.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spoorthiuk/llm-switchboard"
  }
}
